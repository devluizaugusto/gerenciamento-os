POST http://localhost:3001/api/ordens-servico
Content-Type: application/json

{
  "solicitante": "João Silva",
  "ubs": "UBS Central",
  "setor": "Manutenção",
  "descricao_problema": "Problema na impressora",
  "data_abertura": "14/01/2026",
  "status": "aberto"
}

